package Yhxt;

import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.io.IOException;
public class Dwj {
	public int Dwj(String s) throws IOException {
	File wj=new File(s);
	FileReader wj2=new FileReader(wj);
	int x=wj2.read();
	return x;
	
	//wj2.close();
	}
}
